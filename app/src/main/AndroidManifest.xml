<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidl.quizoid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name="com.example.anubhav.quizoid.OpeningActivity"></activity>
        <activity android:name="com.example.anubhav.quizoid.Options" />
        <activity android:name="com.example.anubhav.quizoid.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.anubhav.quizoid.Aptitude"></activity>
    </application>

</manifest>